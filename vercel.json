{
  "functions": {
    "api/*.php": {
      "runtime": "php@8.3.0"
    }
  },
  "routes": [
    { "src": "/upload", "dest": "/api/upload.php" },
    { "src": "/style.css", "dest": "/style.css" },
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*)", "dest": "/api/serve.php?file=$1" }
  ]
}
